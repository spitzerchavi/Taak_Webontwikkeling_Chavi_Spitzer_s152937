<%-include('partials/header')%>

<img src="<%= group.profileImage %>" alt="" width="200" height="200">
<p><%=group.name%></p>
<p><%=group.description%></p>
<p><%=group.numberOfMembers%></p>
<p><%=group.isRemote%></p>
<p><%=group.dateStarted%></p>
<p><%=group.status%></p>
<p><a href="/leaders/<%=group.leader.id%>"><%=group.leader.name%></a></p>
<% for(let i = 0; i<group.numberOfMembers; i++){ %>
    <p><%= group.members[i] %></p>
<% } %>


<%-include('partials/footer')%>